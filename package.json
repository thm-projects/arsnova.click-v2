  "name": "arsnova-click-v2-frontend",
  "version": "2.0.0",
  "license": "GPL",
  "repository": {
    "type": "git",
    "url": "https://git.thm.de/arsnova/arsnova-click-v2-frontend"
  },
  "bugs": {
    "url": "https://github.com/thm-projects/arsnova.click-v2"
  },
  "homepage": "https://arsnova.click",
  "description": "Version 2 of arsnova.click (Frontend WebApp)",
  "scripts": {
    "dependency-check": "npx --ignore-existing madge --circular --extensions ts src",
    "ng": "ng",
    "clean": "rimraf dist",
    "start:DEV": "ng serve --host 192.168.2.101 --port 4210 --aot",
    "start:DEV:LOCAL": "ng serve --port 80 --aot --disable-host-check --host=0.0.0.0",
    "start:WESTERMANN": "ng run frontend:serve-ssr --configuration=westermann",
    "build:PROD": "ng build --prod && ng run frontend:server:production",
    "build:STAGING_THM": "ng build --configuration=thmStaging && ng run frontend:server:thmStaging",
    "build:BETA_THM": "ng build --configuration=thmBeta && ng run frontend:server:thmBeta",
    "build:WESTERMANN": "ng build --configuration=westermann && ng run frontend:server:westermann",
    "copy:piwik:PROD": "cp src/assets/piwik/piwik.prod.js dist/frontend/browser/assets/piwik/piwik.js",
    "copy:piwik:STAGING_THM": "cp src/assets/piwik/piwik.staging.js dist/frontend/browser/assets/piwik/piwik.js",
    "copy:piwik:BETA_THM": "cp src/assets/piwik/piwik.beta.js dist/frontend/browser/assets/piwik/piwik.js",
    "copy:piwik:WESTERMANN": "cp src/assets/piwik/piwik.westermann.js dist/frontend/browser/assets/piwik/piwik.js",
    "copy:images:PROD": "",
    "copy:images:STAGING_THM": "",
    "copy:images:BETA_THM": "npm run copy:images:WESTERMANN",
    "copy:images:WESTERMANN": "cp src/assets/images/logo.westermann.svg dist/frontend/browser/assets/images/logo.svg; cp src/assets/images/logo.westermann.xl.svg dist/frontend/browser/assets/images/logo.xl.svg; cp src/assets/images/logo_transparent.westermann.png dist/frontend/browser/assets/images/logo_transparent.png",
    "copy:PROD": "npm run copy:piwik:PROD; npm run copy:images:PROD",
    "copy:STAGING_THM": "npm run copy:piwik:STAGING_THM; npm run copy:images:STAGING_THM",
    "copy:BETA_THM": "npm run copy:piwik:BETA_THM; npm run copy:images:BETA_THM",
    "copy:WESTERMANN": "npm run copy:piwik:WESTERMANN; npm run copy:images:WESTERMANN",
    "build:PROD:DEPLOY": "npm run build:PROD && npm run purify",
    "build:PROD-STATS": "ng build --prod --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/frontend/browser/stats-es2015.json",
    "test": "ng test --browsers=ChromeHeadless --watch=false --source-map=false --code-coverage",
    "test:DEV": "ng test --browsers=Chrome --karma-config=src/karma.conf.js --source-map=false --code-coverage",
    "test:DEV:HEADLESS": "ng test --browsers=ChromeHeadless --karma-config=src/karma.conf.js --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4201",
    "purify": "node purifycss.js",
    "compress": "gzip dist/frontend/browser/** -r --best --keep",
    "http-startup": "angular-http-server dist/frontend/browser/ -p 4711",
    "prod-test": "npm run build:PROD:DEPLOY && npm run http-startup",
    "prod-test:SSR": "npm run build:SSR && npm run job && npm run start:SSR",
    "job": "npm run job:images; npm run job:link; npm run job:manifest",
    "job:images:logo": "cd dist/frontend/browser/assets/jobs/; node --experimental-modules GenerateImages.mjs --command=generateLogoImages --root",
    "job:images:frontend": "cd dist/frontend/browser/assets/jobs/; node --experimental-modules GenerateImages.mjs --command=generateFrontendPreview --host=http://localhost:4210 --root",
    "job:images": "cd dist/frontend/browser/assets/jobs/; node --experimental-modules GenerateImages.mjs --command=all --host=http://localhost:4000 --root",
    "job:link": "cd dist/frontend/browser/assets/jobs/; node GenerateMetaNodes.js --command=generateLinkImages --baseUrl=http://localhost:4000",
    "job:manifest": "cd dist/frontend/browser/assets/jobs/; node GenerateMetaNodes.js --command=generateManifest --baseUrl=http://localhost:4000",
    "dev:ssr": "ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:ssr": "ng build --prod && ng run frontend:server:production",
    "prerender": "ng run frontend:prerender",
    "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular/animations": "^11.0.0",
    "@angular/common": "^11.0.0",
