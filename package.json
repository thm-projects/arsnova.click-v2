    "@sentry/browser": "5.27.4",
    "@stomp/ng2-stompjs": "8.0.0",
    "angular-svg-icon": "10.0.0",
    "angular-tag-cloud-module": "5.2.0",
    "angular2-hotkeys": "2.2.0",
    "angular2-qrcode": "2.0.3",
    "angulartics2": "10.0.0",
    "bootstrap": "4.5.3",
